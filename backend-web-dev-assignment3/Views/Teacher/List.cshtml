@model List<backend_web_dev_assignment3.Models.Teacher>

@{
    ViewBag.Title = "Teachers List";
}

<h2>Teachers List</h2>
<p>This page should be showing information about all authors</p>

<div class="container">
    <h2>List of Teachers</h2>
    @foreach (var teacher in Model) {
        <div class="row">
            <div>
                <a href="/Teacher/Show/@teacher.teacherid">@teacher.teacherfname @teacher.teacherlname</a>
            </div>
        </div>
    }
</div>

